{
    "name": "vilivala",
    "version": "1.0.0",
    "scripts": {
        "dev": "vuepress dev docs",
        "build": "vuepress build docs",
        "deploy": "bash deploy.sh",
        "editFm": "node utils/editFrontmatter.js",
        "baiduPush": "node utils/baiduPush.js https://zervan.cn && bash baiduPush.sh",
        "publish": "cd ./vilivala && npm publish && cd .. && yarn updateTheme",
        "updateTheme": "yarn remove vuepress-theme-vilivala && rm -rf node_modules && yarn && yarn add vuepress-theme-vilivala -D"
    },
    "license": "MIT",
    "devDependencies": {
        "dayjs": "^1.9.7",
        "inquirer": "^7.1.0",
        "json2yaml": "^1.1.0",
        "vuepress": "^1.9.5",
        "vuepress-plugin-baidu-autopush": "^1.0.1",
        "vuepress-plugin-baidu-tongji": "^1.0.1",
        "vuepress-plugin-comment": "^0.7.3",
        "vuepress-plugin-demo-block": "^0.7.2",
        "vuepress-plugin-fulltext-search": "^2.2.1",
        "vuepress-plugin-one-click-copy": "^1.0.2",
        "vuepress-plugin-reading-time2": "^2.0.0-beta.24",
        "vuepress-plugin-thirdparty-search": "^1.0.2",
        "vuepress-plugin-zooming": "^1.1.7",
        "vuepress-theme-vilivala": "^1.9.4",
        "yamljs": "^0.3.0"
    },
    "dependencies": {
        "yarn": "^1.22.19"
    }
}
